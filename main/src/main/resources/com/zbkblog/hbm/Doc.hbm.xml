<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zbkblog.entity.Doc" table="doc" schema="zbk_blog">
        <id name="id" column="id"/>
        <property name="title" column="title"/>
        <property name="docMd" column="doc_md"/>
        <property name="classifyId" column="classify_id"/>
        <property name="tagId" column="tag_id"/>
        <property name="updateTime" column="update_time"/>
        <property name="favorNumber" column="favor_number"/>
        <property name="openNumber" column="open_number"/>
    </class>
</hibernate-mapping>